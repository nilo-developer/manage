<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B2EBF2">


    <include
        android:id="@+id/include2"
        layout="@layout/activity_main"
        android:layout_width="match_parent"
        android:layout_height="731dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="393dp"
        android:layout_height="63dp"
        android:layout_gravity="fill_vertical"
        android:layout_marginTop="132dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/include2"
        tools:ignore="MissingConstraints">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="30dp"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:background="#FF9800" />


            <com.developer.mtextfield.TextFieldBoxes
                android:id="@+id/text_field_boxes1"
                style="@style/myfont"
                android:layout_width="268dp"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:layout_marginEnd="70dp"
                android:layoutDirection="rtl"
                app:enabled="true"
                app:labelText="شهر"
                app:textCounterColor="@color/black">

                <com.developer.mtextfield.ExtendedEditText
                    android:id="@+id/extended_edit_text1"
                    style="@style/myfont"
                    android:layout_width="130dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:inputType="text"
                    android:textColor="@color/black" />

            </com.developer.mtextfield.TextFieldBoxes>

        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="393dp"
        android:layout_height="63dp"
        android:layout_gravity="fill_vertical"
        android:layout_marginTop="29dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout1"
        tools:ignore="MissingConstraints">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="30dp"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:layout_gravity="center_vertical"
                android:background="#FF9800" />


            <com.developer.mtextfield.TextFieldBoxes
                android:id="@+id/text_field_boxes2"
                style="@style/myfont"
                android:layout_width="277dp"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:layout_marginEnd="70dp"
                android:layoutDirection="rtl"
                app:enabled="true"
                app:labelText="خیابان اصلی"
                app:textCounterColor="@color/black">

                <com.developer.mtextfield.ExtendedEditText
                    android:id="@+id/extended_edit_text2"
                    style="@style/myfont"
                    android:layout_width="130dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:inputType="text"
                    android:textColor="@color/black" />

            </com.developer.mtextfield.TextFieldBoxes>

        </androidx.cardview.widget.CardView>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="393dp"
        android:layout_height="63dp"
        android:layout_gravity="fill_vertical"
        android:layout_marginTop="44dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        tools:ignore="MissingConstraints">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="30dp"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:background="#FF9800" />


            <com.developer.mtextfield.TextFieldBoxes
                android:id="@+id/text_field_boxes3"
                style="@style/myfont"
                android:layout_width="259dp"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:layout_marginEnd="70dp"
                android:layoutDirection="rtl"
                app:enabled="true"
                app:labelText="کوچه"
                app:textCounterColor="@color/black">

                <com.developer.mtextfield.ExtendedEditText
                    android:id="@+id/extended_edit_text3"
                    style="@style/myfont"
                    android:layout_width="130dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:inputType="text"
                    android:textColor="@color/black" />

            </com.developer.mtextfield.TextFieldBoxes>

        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="393dp"
        android:layout_height="63dp"
        android:layout_gravity="fill_vertical"
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        app:layout_constraintVertical_bias="0.064"
        tools:ignore="MissingConstraints">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="30dp"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:background="#FF9800" />


            <com.developer.mtextfield.TextFieldBoxes
                android:id="@+id/text_field_boxes5"
                style="@style/myfont"
                android:layout_width="264dp"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:layout_marginEnd="70dp"
                android:layoutDirection="rtl"
                app:enabled="true"
                app:labelText="پلاک"
                app:textCounterColor="@color/black">

                <com.developer.mtextfield.ExtendedEditText
                    android:id="@+id/extended_edit_text5"
                    style="@style/myfont"
                    android:layout_width="130dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:inputType="number"
                    android:textColor="@color/black" />

            </com.developer.mtextfield.TextFieldBoxes>

        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <com.sanojpunchihewa.glowbutton.GlowButton
            android:id="@+id/btn_add_address"
            style="@style/myfont"
            android:layout_width="120dp"
            android:layout_height="74dp"
            android:layout_marginStart="70dp"
            android:layout_marginBottom="68dp"
            android:text="ثبت"
            android:textColor="@color/black"
            app:buttonColor="#B2EBF2"
            app:cornerRadius="40dp"
            app:glowColor="#FB8C00"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/btn_cancel_common"
            app:layout_constraintHorizontal_bias="0.672"
            app:layout_constraintStart_toStartOf="parent"
            app:pressedGlowSize="25dp"
            app:unpressedGlowSize="20dp"
            tools:ignore="MissingConstraints" />

        <com.sanojpunchihewa.glowbutton.GlowButton
            android:id="@+id/btn_cancel_address"
            style="@style/myfont"
            android:layout_width="120dp"
            android:layout_height="74dp"
            android:layout_marginEnd="70dp"
            android:layout_marginBottom="68dp"
            android:text="انصراف"
            android:textColor="@color/black"
            app:buttonColor="#B2EBF2"
            app:cornerRadius="40dp"
            app:glowColor="#FB8C00"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:pressedGlowSize="25dp"
            app:unpressedGlowSize="20dp"
            tools:ignore="MissingConstraints" />

