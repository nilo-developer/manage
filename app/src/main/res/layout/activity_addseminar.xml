<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B2EBF2"
    android:orientation="vertical"
    tools:context=".activity.AddSeminarActivity">

    <include
        android:id="@+id/include3"
        layout="@layout/activity_main" />


    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="400dp"
        android:layout_height="93dp"
        android:layout_gravity="fill_vertical"
        android:layout_marginTop="70dp"
        android:orientation="vertical"
        style="@style/myfont"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/include3"
        tools:ignore="MissingConstraints">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="30dp"
            app:cardCornerRadius="20dp"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center_vertical"
                android:background="#FB8C00" />


            <com.jaredrummler.materialspinner.MaterialSpinner
                android:id="@+id/spinner1"
                style="@style/myfont"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_gravity="center_vertical"
                android:layoutDirection="rtl"
                android:layout_marginEnd="50dp"
                android:layout_marginStart="20dp"
                android:textColor="@color/black" />

        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="400dp"
        android:layout_height="93dp"
        android:layout_gravity="fill_vertical"
        android:layout_marginTop="150dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/include3"
        tools:ignore="MissingConstraints">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="30dp"
            app:cardMaxElevation="10dp"
            app:cardCornerRadius="20dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center_vertical"
                android:background="#FB8C00" />

            <com.jaredrummler.materialspinner.MaterialSpinner
                android:id="@+id/spinner2"
                style="@style/myfont"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginEnd="50dp"
                android:layout_marginStart="20dp"
                android:layout_gravity="center_vertical"
                android:layoutDirection="rtl"
                android:textColor="@color/black" />

        </androidx.cardview.widget.CardView>

    </LinearLayout>


    <com.sanojpunchihewa.glowbutton.GlowButton
        android:id="@+id/glowButton3"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:padding="30dp"
        android:text="افزودن مکان جدید"
        android:textSize="12sp"
        android:textColor="#0B0A0A"
        style="@style/myfont"
        app:buttonColor="#B2EBF2"
        app:cornerRadius="50dp"
        app:glowColor="#FB8C00"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.183"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7"
        app:pressedGlowSize="25dp"
        app:unpressedGlowSize="20dp"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="400dp"
        android:layout_height="70dp"
        android:layout_gravity="center_vertical"
        android:layout_marginTop="12dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/glowButton3"
        tools:ignore="MissingConstraints">

        <androidx.cardview.widget.CardView
            android:layout_width="340dp"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="30dp"
            app:cardCornerRadius="20dp"
            app:cardElevation="5dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center_vertical"
                android:background="#FB8C00" />

            <TextView
                android:id="@+id/textViewdate"
                style="@style/myfont"
                android:layout_width="200dp"
                android:layout_height="29dp"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:layout_marginEnd="100dp"
                android:hint="تاریخ برگزاری"
                android:inputType="datetime"
                android:layoutDirection="rtl"
                android:textColor="#212121"
                android:textColorHint="#212121"
                android:textSize="17sp"
                tools:ignore="MissingConstraints" />

        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="400dp"
        android:layout_height="70dp"
        android:layout_gravity="fill_vertical"
        android:layout_marginTop="24dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout8"
        tools:ignore="MissingConstraints">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="30dp"
            app:cardCornerRadius="20dp"
            app:cardElevation="5dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="69dp"
                android:layout_gravity="center_vertical"
                android:background="#FB8C00" />


            <com.developer.mtextfield.TextFieldBoxes
                android:id="@+id/text_field_boxes"
                style="@style/myfont"
                android:layout_width="250dp"
                android:layout_height="70dp"
                android:layout_gravity="center_vertical"
                android:layout_marginEnd="80dp"
                android:layoutDirection="rtl"
                android:layout_marginTop="5dp"
                app:enabled="true"
                app:secondaryColor="#212121"
                app:textHelperColor="#212121"
                app:textCounterColor="#212121"
                app:labelText="عنوان سمینار">

                <com.developer.mtextfield.ExtendedEditText
                    android:id="@+id/extended_edit_text"
                    style="@style/myfont"
                    android:layout_width="130dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:inputType="text"
                    android:textColor="#F6F3F3" />

            </com.developer.mtextfield.TextFieldBoxes>

        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_width="400dp"
        android:layout_height="70dp"
        android:layout_gravity="fill_vertical"
        android:layout_marginTop="48dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout11"
        tools:ignore="MissingConstraints">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="30dp"
            app:cardCornerRadius="20dp"
            app:cardElevation="5dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center_vertical"
                android:background="#FB8C00" />


            <CheckBox
                android:id="@+id/simpleCheckBox1"
                style="@style/myfont"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_marginStart="100dp"
                android:text="حضوری"
                android:layout_gravity="center_vertical"
                android:textColor="@color/black"
                tools:ignore="MissingConstraints" />

            <CheckBox
                android:id="@+id/simpleCheckBox2"
                style="@style/myfont"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_marginStart="20dp"
                android:text="آنلاین"
                android:layout_gravity="center_vertical"
                android:textColor="@color/black"
                tools:ignore="MissingConstraints,UnknownId" />


            <TextView
                android:id="@+id/text_field_boxes2"
                style="@style/myfont"
                android:layout_width="120dp"
                android:layout_height="39dp"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="4dp"
                android:layout_marginEnd="200dp"
                android:hint="نوع سمینار"
                android:inputType="datetime"
                android:layoutDirection="rtl"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:gravity="center_vertical"
                tools:ignore="MissingConstraints" />


        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="432dp"
        android:layout_height="68dp"
        android:layout_gravity="fill_vertical"
        android:layout_marginBottom="28dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout10"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="30dp"
            app:cardCornerRadius="15dp"
            android:elevation="3dp"
            app:cardBackgroundColor="#E0E0E0">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="68dp"
                android:layout_gravity="center_vertical"
                android:background="#B2EBF2" />


            <com.sanojpunchihewa.glowbutton.GlowButton
                android:id="@+id/btn_sabt"
                style="@style/myfont"
                android:layout_width="125dp"
                android:layout_height="70dp"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="220dp"
                android:text="ثبت"
                android:textColor="@color/black"
                app:buttonColor="#B2EBF2"
                app:cornerRadius="40dp"
                android:layout_marginBottom="4dp"
                app:glowColor="#FB8C00"
                app:pressedGlowSize="25dp"
                app:unpressedGlowSize="20dp" />

            <com.sanojpunchihewa.glowbutton.GlowButton
                android:id="@+id/btn_enseraf"
                style="@style/myfont"
                android:layout_width="125dp"
                android:layout_height="70dp"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="40dp"
                android:layout_marginBottom="4dp"
                android:text="انصراف"
                android:textColor="@color/black"
                app:buttonColor="#B2EBF2"
                app:cornerRadius="50dp"
                app:glowColor="#FB8C00"
                app:pressedGlowSize="25dp"
                app:unpressedGlowSize="20dp" />


        </androidx.cardview.widget.CardView>
    </LinearLayout>

